{
  "name": "interweave",
  "version": "0.0.1",
  "description": "Interweave components and elements into interpolated React strings.",
  "keywords": [
    "interweave",
    "inject",
    "interpolate",
    "react",
    "string",
    "component",
    "element"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "babel ./src -d ./lib",
    "watch": "babel --watch ./src -d ./lib",
    "lint": "eslint ./src ./tests",
    "mocha": "mocha --opts ./tests/mocha.opts ./tests/*.test.js ./tests/**/*.test.js",
    "test": "npm run build && npm run lint && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/interweave.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/interweave/issues"
  },
  "homepage": "https://github.com/milesj/interweave#readme",
  "peerDependencies": {
    "react": "^15.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.1",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "react": "^15.3.0"
  },
  "engines": {
    "node": "^5.0.0"
  }
}
