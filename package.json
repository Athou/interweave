{
  "name": "interweave",
  "version": "0.0.1",
  "description": "Interweave components and elements into interpolated React strings.",
  "keywords": [
    "interweave",
    "inject",
    "interpolate",
    "react",
    "string",
    "component",
    "element"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "babel ./src -d ./lib",
    "watch": "babel --watch ./src -d ./lib",
    "lint": "eslint ./src ./tests",
    "mocha": "mocha --opts ./tests/mocha.opts ./tests/*.test.js ./tests/**/*.test.js",
    "test": "npm run build && npm run lint && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/interweave.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/interweave/issues"
  },
  "homepage": "https://github.com/milesj/interweave#readme",
  "peerDependencies": {
    "react": "^15.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "react": "^15.2.1"
  },
  "engines": {
    "node": "^5.0.0"
  }
}
